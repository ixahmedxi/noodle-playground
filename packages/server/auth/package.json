{
  "name": "@noodle/auth",
  "version": "0.1.0",
  "description": "Noodle's next-auth configuration package",
  "author": "Ahmed Elsakaan",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "react": [
        "dist/react.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint --ext .ts,.js,.cjs .",
    "lint:fix": "eslint --ext .ts,.js,.cjs . --fix",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@noodle/tsconfig": "workspace:*",
    "eslint-config-noodle": "workspace:*",
    "tsup": "^6.6.3"
  },
  "dependencies": {
    "@noodle/database": "workspace:*",
    "@next-auth/prisma-adapter": "^1.0.5",
    "next-auth": "^4.20.1"
  }
}
